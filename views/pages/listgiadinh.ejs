

  <% if(typeof message != 'undefined') { %><h3><%- undefined %></h3><% } %>

  <div class="col-lg-12">
      <section class="panel">
        <header class="panel-heading">
         DANH SÁCH SẢN PHẨM GIA ĐÌNH
          <div>
            <a class="btn btn-primary" href="/page/giadinh"><i class="icon_plus_alt2"></i>Thêm sản phẩm</a>
          </div>
        </header>
        <table class="table table-striped table-advance table-hover">    
            <thead>
            <tr>
              <th> Order</th>
              <th><i class="icon_profile"></i> Name</th>
              <th> EntryPrice</th>
              <th> ExportPrice</th>
              <th> Number</th>
              <th> Category</th>
              <th> Image</th>
              <th>EntryDate</th>
              <th> Description</th>
              <th></th>
            </tr>
          </thead>
          <tbody>
              <% giadinhs.forEach(function(ht){ %>
                
                <tr>
                    <td>#<%- ht._id %></td>
                    <td><%- ht.name %></td>
                    <td><%- ht.entryprice %></td>
                    <td><%- ht.exportprice %></td>
                    <td><%- ht.number %></td>
                    <td><%- ht.category %></td>
                    <td><img width="150px" src="../upload/giadinh/<%- ht.image %>"></td>
                    <td><%- ht.date %></td>
                    <td><%- ht.description %></td>
                    <td>
                      <div class="btn-group">
                        
                        <a class="btn btn-success" href="/page/editgiadinh/<%- ht._id %>"><i class="icon_check_alt2"></i></a>
              <a class="btn btn-danger" onclick ="return confirm('Are you sure')" href="/page/giadinh/delete/<%- ht._id %>"><i class="icon_close_alt2"></i></a>
                      </div>
                    </td>
                  </tr> 
                 
                  <% }) %> 
          </tbody>
        </table>
      </section>
    </div>