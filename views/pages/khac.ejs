


<div class="col-lg-12">
    <section class="panel">
      <header class="panel-heading">
        THÊM SẢN PHẨM KHÁC
      </header>
      <div class="panel-body">
        <div class="form">
          <form class="form-validate form-horizontal" id="feedback_form" method="post" enctype="multipart/form-data" action="">
            <div class="form-group ">
              <label for="cname" class="control-label col-lg-2"> Tên sản phẩm </span></label>
              <div class="col-lg-10">
                <input class="form-control"  name="txtName" minlength="2" type="text" />
              </div>
            </div>
            <div class="form-group ">
                <label for="cname" class="control-label col-lg-2">Order</label>
                <div class="col-lg-10">
                  <input class="form-control" id="subject" name="order" placeholder="id" minlength="5" type="password" />
                </div>
              </div>
            <div class="form-group ">
              <label for="cemail" class="control-label col-lg-2">Giá nhập </span></label>
              <div class="col-lg-10">
                <input class="form-control " id="cemail" type="txt" name="txtEntryprice"  />
              </div>
            </div>
            <div class="form-group ">
              <label for="curl" class="control-label col-lg-2">Giá bán</label>
              <div class="col-lg-10">
                <input class="form-control " id="curl" type="text" name="txtExportprice" />
              </div>
            </div>
            <div class="form-group ">
              <label for="cname" class="control-label col-lg-2">Số lượng </label>
              <div class="col-lg-10">
                <input class="form-control" id="subject" name="txtNumber" minlength="2" type="text" />
              </div>
            </div>
            <div class="form-group ">
              <label  class="control-label col-lg-2">Loại sản phẩm</label>
              <div class="col-lg-10">
                <select name="selectCate" class="form-control m-bot15">
                    <% cats.forEach(function(cat){ %>
                      <option value="<%- cat.name %>" ><%- cat.name %></option>   
                      <% }) %>             
                </select>
            </div>
            </div>
            <div class="form-group">
                <label class="control-label col-lg-2">Hình ảnh</label>
                <div class="col-lg-10">
                  <input type="file"  name="fileImage" size="6">
                </div>
            </div>
            <div class="form-group ">
              <label  for="ccomment" class="control-label col-lg-2"> Ngày nhập</label>
              <div class="col-lg-10">
                <input id="dp1" type="text"  size="16" name="txtDate" class="form-control">
              </div>
            </div>
            <div class="form-group ">
                <label for="ccomment" class="control-label col-lg-2">Mô tả</label>
                <div class="col-lg-10">
                  <textarea class="form-control ckeditor" name="txtPdfurl" rows="5"></textarea>
                </div>
            </div>
            <div class="form-group">
              <div class="col-lg-offset-2 col-lg-10">
                <button class="btn btn-primary" type="submit">Save</button>
                <button class="btn btn-default" type="button">Cancel</button>
              </div>
            </div>
          </form>
        </div>
      </div>
    </section>
  </div>

  <% if(typeof message != 'undefined') { %><h3><%- undefined %></h3><% } %>
  
  
 